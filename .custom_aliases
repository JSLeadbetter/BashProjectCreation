function create() {
    cd ~/Desktop/Programming/Projects/
    mkdir $1
    cd $1
    python3 ~/python_scripts/create.py $1
    git init
    git remote add origin https://github.com/JSLeadbetter/$1.git
    git remote -v
    touch README.md
    git add .
    git commit -m "Initial commit"
    git push -u origin master
    code .
}